{
  "name": "dash",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "bin/server-run.sh",
    "dev": "concurrently -k \"vite\" \"tsx watch src/server.ts\"",
    "build": "vite build && tsc -p tsconfig.server.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "screenshot": "tsx src/command.ts screenshot output.png",
    "display-update": "npm run screenshot && bin/display-update output.png"
  },
  "dependencies": {
    "@fontsource-variable/roboto-condensed": "^5.1.1",
    "@fontsource/roboto": "^5.1.1",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "install": "^0.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.17.14",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "lodash": "^4.17.21",
    "puppeteer-core": "^24.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.9"
  }
}
